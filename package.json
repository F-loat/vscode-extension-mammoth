{
	"name": "vscode-extension-mammoth",
	"displayName": "Mammoth",
	"description": "",
	"version": "0.0.1",
	"publisher": "F-loat",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/F-loat/vscode-extension-mammoth.git"
	},
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"main": "./dist/extension",
	"icon": "media/icon.png",
	"activationEvents": [
		"onCommand:mammoth.editor",
		"onCustomEditor:mammoth.editor"
	],
	"contributes": {
		"commands": [
			{
				"command": "mammoth.editor",
				"title": "Mammoth Editor"
			},
			{
				"command": "mammoth.transform",
				"title": "%mammoth.menu%"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "resourceExtname == .docx",
					"command": "mammoth.transform",
					"group": "0_mammoth"
				}
			]
		},
		"customEditors": [
			{
				"viewType": "mammoth.editor",
				"displayName": "Mammoth Editor",
				"selector": [
					{
						"filenamePattern": "*.docx"
					}
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"test-compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.32.0",
		"@typescript-eslint/eslint-plugin": "^4.16.0",
		"@typescript-eslint/parser": "^4.16.0",
		"eslint": "^7.21.0",
		"ts-loader": "^7.0.5",
		"typescript": "^4.4.3",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11"
	},
	"dependencies": {
		"js-beautify": "^1.14.0",
		"mammoth": "^1.4.19"
	}
}
